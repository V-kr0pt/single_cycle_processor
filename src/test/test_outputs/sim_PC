#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5cc56c3b8dc0 .scope module, "PC_testbench" "PC_testbench" 2 1;
 .timescale 0 0;
v0x5cc56c406420_0 .var "clk", 0 0;
v0x5cc56c4064e0_0 .var "load", 0 0;
v0x5cc56c4065b0_0 .net "points", 7 0, v0x5cc56c4060a0_0;  1 drivers
v0x5cc56c4066b0_0 .var "reset", 0 0;
v0x5cc56c406780_0 .var "set_value", 7 0;
S_0x5cc56c3b8f50 .scope module, "point_counter" "PointCounter" 2 13, 3 1 0, S_0x5cc56c3b8dc0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "load";
    .port_info 3 /INPUT 8 "set_value";
    .port_info 4 /OUTPUT 8 "points";
v0x5cc56c3baba0_0 .net "clk", 0 0, v0x5cc56c406420_0;  1 drivers
v0x5cc56c405fe0_0 .net "load", 0 0, v0x5cc56c4064e0_0;  1 drivers
v0x5cc56c4060a0_0 .var "points", 7 0;
v0x5cc56c406190_0 .net "reset", 0 0, v0x5cc56c4066b0_0;  1 drivers
v0x5cc56c406250_0 .net "set_value", 7 0, v0x5cc56c406780_0;  1 drivers
E_0x5cc56c3f3770 .event posedge, v0x5cc56c406190_0, v0x5cc56c3baba0_0;
    .scope S_0x5cc56c3b8f50;
T_0 ;
    %wait E_0x5cc56c3f3770;
    %load/vec4 v0x5cc56c406190_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5cc56c4060a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5cc56c405fe0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5cc56c406250_0;
    %assign/vec4 v0x5cc56c4060a0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5cc56c4060a0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5cc56c4060a0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5cc56c3b8dc0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5cc56c406420_0;
    %inv;
    %store/vec4 v0x5cc56c406420_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5cc56c3b8dc0;
T_2 ;
    %vpi_call 2 27 "$dumpfile", "test_PC.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5cc56c3b8dc0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc56c406420_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc56c4066b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc56c4064e0_0, 0, 1;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x5cc56c406780_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc56c4066b0_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc56c4064e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc56c4064e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc56c4066b0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 47 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
